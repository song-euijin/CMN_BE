<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmn.service.impl.CodeMapper">

	<resultMap id="code" type="com.cmn.service.CodeVO">
	</resultMap>

	<select id="selectCodeList" resultMap="code" >
		select * from tb_common_code where ${searchKey} like '%${searchValue}%' order by common_code
	</select>
</mapper>	 